{
  "name": "GudFood Filter",
  "description": "GudFood Filter Extension",
  "version": "2.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "favicon.png"
  },
  "permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://orders.gudfood.com.ua/order"
      ],
      "js": [
        "jquery.min.js",
        "jquery-ui.min.js",
        "main.js"
      ],
      "css": [
        "jquery-ui.css"
      ],
      "run_at": "document_end"
    }
  ]
}
