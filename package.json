{
  "name": "gudfood-filter",
  "version": "1.0.0",
  "description": "GudFood Filter extension",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "pack": "node pack.js",
    "repack": "npm run build && npm run pack"
  },
  "keywords": [
    "chrome",
    "extension",
    "gudfood"
  ],
  "author": "Dmitry Teslitsky <dmitryteslitsky@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.254",
    "@types/jest": "29.5.11",
    "@types/jquery": "3.5.29",
    "@types/jqueryui": "1.12.21",
    "adm-zip": "0.5.10",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.55.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-security": "1.7.1",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "clearMocks": true
  }
}
